{
  "command": {
    "publish": {
      "ignoreChanges": [
        ".gitignore",
        "*.md",
        "docs/",
        "node_modules/",
        ".github/"
      ],
      "message": "chore(release): publish"
    },
    "bootstrap": {
      "npmClientArgs": ["--no-package-lock"]
    }
  },
  "npmClient": "npm",
  "packages": ["packages/*"],
  "version": "0.0.1"
}
