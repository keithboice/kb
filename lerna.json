{
  "command": {
    "create": {
      "config": {
        "access": "public",
        "scope": "@kb"
      },
      "directories": {
        "lib": "lib",
        "test": "__tests__"
      },
      "engines": {
        "node": ">= 6.9.0"
      },
      "files": [
        "index.js",
        "lib"
      ],
      "homepage": "https://github.com/keithboice/kb",
      "license": "ISC",
      "main": "index.js",
      "publishConfig": {
        "access": "public"
      },
      "repository": {
        "type": "git",
        "url": "git+https://github.com/keithboice/kb.git",
        "directory": "packages/name"
      }
    },
    "publish": {
      "ignoreChanges": [
        "**/__fixtures__/**",
        "**/__tests__/**",
        "**/*.md",
        "docs/"
      ],
      "message": "chore(release): publish"
    },
    "version": {
      "allowBranch": "main",
      "conventionalCommits": true,
      "message": "chore(release): %s"
    },
    "bootstrap": {
      "npmClientArgs": ["--no-package-lock"]
    }
  },
  "npmClient": "npm",
  "packages": ["packages/*"],
  "version": "0.0.6"
}
