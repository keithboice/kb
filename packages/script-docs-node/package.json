{
	"name":                "@kb/script-docs-node",
	"version":             "0.0.1",
	"description":         "Nodejs jsdoc script for kb projects",
	"homepage":            "https://github.com/keithboice/kb",
	"repository":          {
		"type": "git",
		"url":  "https://github.com/keithboice/kb.git"
	},
	"license":             "MIT",
	"author":              "Keith Boice <69816300+keithboice@users.noreply.github.com>",
	"main":                "index.js",
	"module":              "index.mjs",
	"types":               "commonjs",
	"bin":                 {
		"docs": "cli.js"
	},
	"files":               [
		"dist/**"
	],
	"scripts":             {
		"build":   "build ./",
		"commit:": "commit ./",
		"deploy":  "deploy ./",
		"docs":    "jsdoc -c ./conf.js --recurse",
		"reports": "reports ./",
		"lint":    "lint ./",
		"release": "release ./",
		"test":    "test ./"
	},
	"dependencies":        {
		"app-root-path":     "^3.0.0",
		"child_process":     "^1.0.2",
		"jsdoc":             "^3.6.6",
		"path":              "^0.12.7",
		"shelljs":           "*",
		"yargs":             "^16.2.0",
		"jsdoc-to-markdown": "^6.0.1"
	},
	"bundledDependencies": [
		"jsdoc",
		"jsdoc-to-markdown",
		"yargs",
		"child-process",
		"app-root-path"
	],
	"engines":             {
		"node": ">=14.0.0",
		"pnpm": ">=5"
	},
	"languageName":        "node"
}
