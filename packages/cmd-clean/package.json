	"main": "./bin.js",
	"name": "@kb/cmd-clean",
	"publishConfig": {},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/keithboice/kb.git"
	},
	"scripts": {
		"clean": "eslint --quiet --color --cache --fix .",
		"clean:all": "eslint --quiet --color --cache --fix .",
		"clean:css": "stylelint --cache --color --quiet --fix '**/*.{css,scss,sass}'",
		"clean:js": "eslint --quiet --color --cache --env node --ext .js,.cjs,.mjs --fix .",
		"clean:pretty": "yarn dlx prettier --ignore-unknown --write .",
		"clean:es": "eslint --quiet --color --cache --fix .",
		"clean:pkg": "prettier --ignore-unknown --write package.json '**/package.json'"
	},
	"version": "0.0.0-development"
}
