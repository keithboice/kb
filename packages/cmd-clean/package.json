	"scripts": {
		"clean:all": "eslint --config lib/.eslintrc.js --ignore-path lib/.cleanignore --quiet --color --cache --fix .",
		"clean:css": "stylelint --config lib/.stylelintrc.js --cache --color --ignore-path lib/.cleanignore --quiet --fix '**/*.{css,scss,sass}'",
		"clean:js": "eslint --config lib/js.eslintrc.js --ignore-path lib/.cleanignore --quiet --color --cache --env node --ext .js,.cjs,.mjs --fix tests/files",
		"clean:pretty": "yarn dlx prettier --config packages/clean/lib/.prettierrc.js --ignore-path packages/clean/lib/.cleanignore --ignore-unknown --write . ",
		"clean:es": "eslint --config lib/.eslintrc.js --ignore-path lib/.cleanignore --quiet --color --cache --fix . ",
		"clean:pkg": "prettier --config ./lib/.prettierrc.js --ignore-path lib/.cleanignore --ignore-unknown --write package.json '**/package.json'"
	},
	"version": "0.0.6"
