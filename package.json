{
	"author": "Keith Boice <keith.boice.work@gmail.com> (https://github.com/keithboice)",
	"bugs": {
		"url": "https://github.com/keithboice/kb/issues"
	},
	"description": "kb packages",
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"homepage": "https://keithboice.github.io/kb/",
	"keywords": [
		"kb"
	],
	"license": "MIT",
	"main": "start.js",
	"name": "kb",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/keithboice/kb.git"
	},
	"scripts": {
		"commit": "cz -a",
		"push": "git push",
		"stage": "yarn stage --commit",
		"install": "yarn install",
		"pack": "yarn pack",
		"git:push": "git push"
	},
	"resolutions": {
		"@kb/cmd-commit": "workspace:packages/cmd-commit",
		"@kb/config-eslint": "workspace:packages/config-eslint",
		"@kb/config-jest": "workspace:packages/config-jest",
		"@kb/config-prettier": "workspace:packages/config-prettier",
		"@kb/config-stylelint": "workspace:packages/config-stylelint"
	},
	"version": "0.0.0",
	"workspaces": [
		"packages/*"
	],
	"dependencies": {
		"@kb/cmd-commit": "workspace:packages/cmd-commit",
		"@kb/config-eslint": "workspace:packages/config-eslint",
		"@kb/config-jest": "workspace:packages/config-jest",
		"@kb/config-prettier": "workspace:packages/config-prettier",
		"@kb/config-stylelint": "workspace:packages/config-stylelint",
		"@yarnpkg/cli": "^2.4.0",
		"@yarnpkg/core": "^2.4.0",
		"@yarnpkg/plugin-workspace-tools": "^2.2.0"
	}
}
