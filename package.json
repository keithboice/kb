{
  "author": "Keith Boice <keith.boice.work@gmail.com> (https://github.com/keithboice)",
  "bugs": {
    "url": "https://github.com/keithboice/kb/issues"
  },
  "dependencies": {
    "@kb/eslint-config-kb": "file:packages/eslint-config-kb",
    "@kb/grunt": "file:packages/grunt",
    "@kb/jest-config-kb": "file:packages/jest-config-kb",
    "@kb/prettier-plugin-kb": "file:packages/prettier-plugin-kb"
  },
  "description": "kb packages",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "lerna-changelog": "^1.0.1",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-kb"
    ]
  },
  "homepage": "https://keithboice.github.io/kb/",
  "jest": {
    "preset": "@kb/jest-config-kb"
  },
  "keywords": [
    "kb"
  ],
  "license": "MIT",
  "main": "index.js",
  "name": "kb",
  "prettier": "prettier-plugin-kb",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/keithboice/kb.git"
  },
  "scripts": {
    "add": "lerna add --dev",
    "bootstrap": "npm-run-all lerna:link lerna:bootstrap",
    "modified:dependency": "npm-run-all bootstrap",
    "modified:js": "npm-run-all clean:lint clean:p",
    "clean:lint": "eslint -c \"packages/eslint-config-kb/lib/.eslintrc.yaml\" --resolve-plugins-relative-to \"packages/eslint-config-kb\"  --fix --quiet --cache --ignore-path .gitignore .",
    "clean:p": "prettier --write --ignore-unknown --ignore-path .gitignore .",
    "lerna:bootstrap": "lerna bootstrap --hoist",
    "lerna:create": "lerna create",
    "lerna:link": "lerna link convert",
    "test": "jest",
    "test:coverage": "jest --coverage --no-cache"
  },
  "version": "0.0.6"
}
