{
  "author": "Keith Boice <keith.boice.work@gmail.com> (https://github.com/keithboice)",
  "bugs": {
    "url": "https://github.com/keithboice/kb/issues"
  },
  "description": "kb packages",
  "engines": {
    "node": ">=14.0.0"
  },
  "eslint": {
    "extends": "@kb/clean"
  },
  "homepage": "https://keithboice.github.io/kb/",
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "keywords": [
    "kb"
  ],
  "license": "MIT",
  "main": "start.js",
  "name": "kb",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/keithboice/kb.git"
  },
  "resolutions": {
    "@kb/build": "workspace:packages/build",
    "@kb/clean": "workspace:packages/clean",
    "@kb/hook-post-install": "workspace:packages/hook-post-install",
    "@kb/hook-pre-commit": "workspace:packages/hook-pre-commit",
    "@kb/make-docs": "workspace:packages/make-docs",
    "@kb/plugin": "workspace:packages/plugin",
    "@kb/run": "workspace:packages/run",
    "@kb/templates": "workspace:packages/templates",
    "@kb/test": "workspace:packages/test",
    "@kb/version": "workspace:packages/version",
    "@kb/yo": "workspace:packages/yo"
  },
  "scripts": {
    "pkg:clean": "node packages/clean",
    "pkg:make-docs": "node packages/make-docs",
    "pkg:test": "node packages/test",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:coverage": "jest --coverage --no-cache",
    "semantic-release": "semantic-release"
  },
  "version": "0.0.0-development",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@yarnpkg/builder": "2.1.3",
    "@yarnpkg/core": "2.4.0",
    "@yarnpkg/doctor": "2.1.3",
    "@yarnpkg/eslint-config": "0.2.0",
    "@yarnpkg/plugin-constraints": "2.2.0",
    "@yarnpkg/plugin-essentials": "2.4.0",
    "@yarnpkg/plugin-git": "2.3.0",
    "@yarnpkg/plugin-github": "2.1.2",
    "@yarnpkg/plugin-link": "2.1.1",
    "@yarnpkg/plugin-node-modules": "2.3.0",
    "@yarnpkg/plugin-pack": "2.2.3",
    "@yarnpkg/plugin-patch": "2.1.2",
    "@yarnpkg/plugin-pnp": "2.4.0",
    "@yarnpkg/plugin-workspace-tools": "2.2.0",
    "@yarnpkg/pnpify": "2.4.0",
    "semantic-release": "^17.3.7"
  },
  "dependencies": {
    "@kb/build": "workspace:*",
    "@kb/clean": "workspace:*",
    "@kb/hook-post-install": "workspace:*",
    "@kb/hook-pre-commit": "workspace:*",
    "@kb/make-docs": "workspace:*",
    "@kb/run": "workspace:*",
    "@kb/templates": "workspace:*",
    "@kb/test": "workspace:*",
    "@kb/version": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@yarnpkg/cli": "^2.4.0",
    "eslint": "^7.19.0"
  }
}
